{
  "name": "fsa_vite_express_template",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start:dev": "nodemon --exec \"node --env-file=server/.env\" server/index.js",
    "db:schema": "node --env-file=server/.env -e \"require('child_process').execSync('psql \\\"' + process.env.DATABASE_URL + '\\\" -f server/db/schema.sql', { stdio: 'inherit' })\"",
    "db:seed": "node --env-file=server/.env server/db/seedRunner.js",
    "db:reset": "npm run db:schema && npm run db:seed"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "^3.1.11"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "pg": "^8.16.3",
    "uuid": "^13.0.0"
  }
}
